<?xml version="1.0"?>
<package format="2">
  <name>aws_robomaker_bookstore_world</name>
  <version>1.0.0</version>
  <description>
    AWS RoboMaker package for a bookstore with sheling and tables Gazebo simulations.
  </description>
  <license>Apache 2.0</license>
  <author email="ros-contributions@amazon.com">AWS RoboMaker</author>
  <maintainer email="ros-contributions@amazon.com">AWS RoboMaker</maintainer>
  <buildtool_depend>catkin</buildtool_depend>

  <depend>gazebo_ros</depend>
  <depend>turtlebot3_description</depend>
  <depend>turtlebot3_navigation</depend>

  <exec_depend>gazebo</exec_depend>
  <exec_depend>gazebo_plugins</exec_depend>
  <exec_depend>turtlebot3_gazebo</exec_depend>

  <!-- SLAM -->
  <!--
  <exec_depend>turtlebot3_slam</exec_depend>
  <exec_depend>hector_mapping</exec_depend>
  -->

  <export>
    <gazebo_ros plugin_path="${prefix}/lib" gazebo_media_path="${prefix}" gazebo_model_path="${prefix}/models"/>
  </export>
</package>
